<Window x:Class="LinkExtractor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LinkExtractor"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Icon="C:\Users\creat\source\repos\LinkExtractor\AppIcon.png"
        Title="LinkExtractor" Height="455" Width="815">
    <Grid Background="#FF4A2E98">
        <Button x:Name="ExtractLinks" Content="Parse" HorizontalAlignment="Left" Margin="633,10,0,0" Width="157" Click="ExtractLinks_Click" Foreground="#FFB4B4B4" Height="28" VerticalAlignment="Top" Background="#FF5F3CC0" FontFamily="Cascadia Mono" FontSize="16"/>
        <TextBox x:Name="UrlInput" VerticalContentAlignment="Center" Style="{StaticResource placeHolder}" Tag="Link in format http:AAA.AAA" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Width="618" Height="28" VerticalAlignment="Top" Background="#FF5F3CC0" Foreground="#FFB4B4B4" FontFamily="Cascadia Mono" FontSize="16" CaretBrush="#FFB4B4B4"/>
        <TextBox x:Name="LinkOutput" HorizontalAlignment="Left" Margin="10,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="780" Height="364" Foreground="#FF00FF32" Background="Black" FontFamily="Cascadia Mono" FontSize="16"/>
    </Grid>
</Window>
